{
    "name": "gmt-property-management/cms",
    "type": "project",
    "description": "GMT - Commercial Management System",
    "keywords": [],
    "license": "private",
    "repositories": [
        {
            "type": "path",
            "url": "./nova"
        },
        {
            "type": "path",
            "url": "./nova-components/NovaCmsTheme"
        },
        {
            "type": "github",
            "url": "https://github.com/muhghazaliakbar/advanced-nova-media-library.git"
        }
    ],
    "require": {
        "php": "^7.2",
        "benjaminhirsch/nova-slug-field": "^1.2",
        "cloudcake/nova-fixed-bars": "^1.1",
        "ebess/advanced-nova-media-library": "^3.2.0",
        "epartment/nova-dependency-container": "^1.2",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "gmt/nova-cms-theme": "*",
        "gregoriohc/laravel-nova-theme-responsive": "^0.8.1",
        "guzzlehttp/guzzle": "^6.3",
        "kabbouchi/nova-impersonate": "^1.2",
        "laravel/framework": "^7.0",
        "laravel/helpers": "^1.2",
        "laravel/nova": "*",
        "laravel/sanctum": "^2.3",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.0",
        "orlyapps/nova-belongsto-depend": "^2.0",
        "rimu/formatted-number": "^0.2.0",
        "spatie/laravel-medialibrary": "^8.0.0",
        "titasgailius/search-relations": "^1.0"
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
